{
  "name": "n8n-nodes-google-chat-cards-v2",
  "version": "0.2.2",
  "description": "n8n node for creating rich Google Chat Cards v2 messages with visual builder",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "google-chat",
    "cards",
    "messaging",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jezweb/n8n-nodes-google-chat-cards-v2",
  "author": {
    "name": "Jeremy Dawes",
    "email": "jeremy@jezweb.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jezweb/n8n-nodes-google-chat-cards-v2.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && cp -r ./src/nodes/GoogleChatCardsV2/*.svg ./dist/nodes/GoogleChatCardsV2/ 2>/dev/null || : && cp -r ./src/nodes/GoogleChatCardsV2/*.png ./dist/nodes/GoogleChatCardsV2/ 2>/dev/null || :",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts",
    "lintfix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "n8n-node-dev": "n8n-node-dev",
    "test": "n8n-node-dev build --watch",
    "start": "n8n-node-dev run"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/GoogleChatCardsV2/GoogleChatCardsV2.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "n8n-workflow": "^1.28.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.28.0"
  }
}
